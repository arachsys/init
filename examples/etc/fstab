sysfs /sys sysfs nodev,noexec 0 0
cgroup2 /sys/fs/cgroup cgroup2 memory_recursiveprot,nodev,noexec,nsdelegate 0 0
tmpfs /tmp tmpfs mode=1777,nodev,nosuid 0 0
